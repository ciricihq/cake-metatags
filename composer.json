{
    "name": "ciricihq/cake-metatags",
    "description": "Metatags plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "~2.6.0",
        "ciricihq/cirici-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Cirici\\Metatags\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cirici\\Metatags\\App\\": "tests/app/src",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "App\\": "tests/App"
        }
    }
}
